{
  "name": "quick-go-to-selected-terraform-path",
  "displayName": "Go To Path For Terraform Module",
  "description": "Fill the selected file path to quickOpen panel.",
  "version": "0.0.7",
  "publisher": "duch11",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.quickGoToSelectedFilePath"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.quickGoToSelectedFilePath",
        "title": "Quick Go To Selected File Path",
        "category": "Quick"
      }
    ],
    "keybindings": [
      {
        "command": "extension.quickGoToSelectedFilePath",
        "key": "ctrl+e",
        "mac": "cmd+e"
      }
    ]
  },
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^7.10.9",
    "typescript": "^2.9.2",
    "vscode": "^1.1.36"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duch11/quick-go-to-selected-terraform-path"
  },
  "icon": "images/icon.png"
}
