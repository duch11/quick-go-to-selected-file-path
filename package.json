{
    "name": "quickPick-selected",
    "displayName": "quickPick-selected-text",
    "description": "自动将选取的文本填充到 quickOpen 面板，用于快速跳转文件",
    "version": "0.0.1",
    "publisher": "duXing",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.quickPickSelectedText"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.quickPickSelectedText",
                "title": "Quick Pick Selected Text",
                "category": "sigma"
            }
        ]
    },
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
