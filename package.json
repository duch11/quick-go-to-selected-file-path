{
    "name": "quick-go-to-selected-file-path",
    "displayName": "Quick Go To Selected File Path",
    "description": "Fill the selected file path to quickOpen panel.（自动将选取的文本填充到 quickOpen 面板，用于快速跳转文件。）",
    "version": "0.0.6",
    "publisher": "duXing",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.quickGoToSelectedFilePath"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.quickGoToSelectedFilePath",
                "title": "Quick Go To Selected File Path",
                "category": "Quick"
            }
        ],
        "keybindings": [
            {
                "command": "extension.quickGoToSelectedFilePath",
                "key": "ctrl+e",
                "mac": "cmd+e"
            }
        ]
    },
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "typescript": "^2.6.1",
        "vscode": "^1.1.22"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/g8up/quick-go-to-selected-file-path"
    },
    "icon": "images/icon.png"
}
